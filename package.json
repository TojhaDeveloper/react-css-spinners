{
  "name": "react-css-spinners",
  "version": "1.0.0",
  "description": "CSS-only  spinners for React",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.js\" ",
    "lint:fix": "prettier-eslint \"**/*.js\" --write "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "lint-staged": {
      "*.js": [
        "npm run lint:fix",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TojhaDeveloper/react-css-spinners.git"
  },
  "keywords": [
    "react",
    "spinners",
    "css"
  ],
  "author": "Tuheen Ojha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TojhaDeveloper/react-css-spinners/issues"
  },
  "homepage": "https://github.com/TojhaDeveloper/react-css-spinners#readme",
  "devDependencies": {
    "eslint": "^5.15.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
